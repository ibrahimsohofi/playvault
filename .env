# API Keys for services (keeping for potential future use)
# Add your API keys here as needed

# Application Settings
REACT_APP_ENVIRONMENT=development  # Change to 'production' for production
REACT_APP_DEBUG_MODE=true         # Set to false in production
REACT_APP_VERSION=1.0.0

# API Endpoints (if applicable)
REACT_APP_API_BASE_URL=https://api.yourservice.com/v1
REACT_APP_CDN_URL=https://cdn.yourservice.com

# Feature Flags
# Add feature flags here as needed

# Rate Limiting (requests per minute)
REACT_APP_API_RATE_LIMIT=60

# Security Settings
REACT_APP_STRICT_MODE=true
REACT_APP_REQUIRE_HTTPS=true
